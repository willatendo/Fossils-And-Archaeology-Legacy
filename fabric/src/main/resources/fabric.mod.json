{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "accessWidener": "fossilslegacy.accesswidener",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${issue_tracker}"
  },
  "license": "${license}",
  "icon": "${mod_id}_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "willatendo.fossilslegacy.FossilsLegacyFabricMod"
    ],
    "client": [
      "willatendo.fossilslegacy.client.FossilsLegacyFabricClient"
    ],
    "jei_mod_plugin": [
      "willatendo.fossilslegacy.server.jei.FossilsLegacyJEI"
    ],
    "modmenu": [
      "willatendo.fossilslegacy.client.FossilsLegacyModMenu"
    ]
  },
  "mixins": [
    "fossilslegacy.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${java_version}",
    "fabric-api": "*"
  }
}